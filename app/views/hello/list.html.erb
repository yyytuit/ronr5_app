<table>
  <tr>
    <th>ISBNコード</th><th>署名</th><th>価格</th>
    <th>出版社</th><th>刊行日</th><th>ダウンロード</th>
  </tr>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.isbn %></td>
      <td><%= book.title %></td>
      <td><%= book.price %></td>
      <td><%= book.publish %></td>
      <td><%= book.published %></td>
      <td><%= book.dl %></td>
    </tr>
  <% end %>
</table>
